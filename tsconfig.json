{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "resolveJsonModule": true,
    "baseUrl": "./src",
    "paths": {
      "@styles/*": ["styles/*"],
      "@components/*": ["components/*"],
      "@layouts/*": ["layouts/*"],
      "@utils/*": ["utils/*"],
      "@i18n/*": ["i18n/*"],
      "@types/*": ["types/*"]
    }
  }
}
